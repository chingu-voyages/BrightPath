{
    "name": "backend",
    "version": "1.0.0",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/server.js",
        "dev": "nodemon src/server.ts",
        "docker:up": "docker compose up -d",
        "docker:down": "docker compose down",
        "prisma:migration": "npx prisma migrate dev --name init",
        "prisma:generate": "npx prisma generate",
        "generate:dummy": "npx ts-node scripts/dummy-data.ts",
        "generate:mock": "npx ts-node scripts/generate-mock-data.ts && cp -f ./mockData.json ../frontend/tests/mocks/data.json",
        "test": "npm run docker:up && npx prisma migrate deploy && jest -i",
        "test:watch": "npm run docker:up && npx prisma migrate deploy && jest --watch",
        "prettier": "prettier --write ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@faker-js/faker": "^9.4.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.10.5",
        "@types/supertest": "^6.0.2",
        "concurrently": "^9.1.2",
        "jest": "^29.7.0",
        "jest-mock-extended": "^4.0.0-beta1",
        "nodemon": "^3.1.9",
        "prettier": "3.4.2",
        "prisma": "^6.2.1",
        "supertest": "^7.0.0",
        "test": "^3.3.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3"
    },
    "dependencies": {
        "@prisma/client": "^6.2.1",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "multer": "^1.4.5-lts.1"
    }
}
